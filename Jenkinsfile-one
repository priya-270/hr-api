@Library('jhc') _
pipeline {
    agent any

    stages {
        stage('gitchekout') {
            steps {
                git branch: "{main}", credentialsId: 'github-tokens', url: 'https://github.com/priya-270/hr-api'
            }
        }
        stage('Maven Build') {
            steps {
                sh 'mvn clean package'
            }
        }
        stage('Tomcat Deploy') {
            steps {
              tomcatdeploy()
        }
        
    }
}
    
